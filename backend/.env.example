# Copy this file to .env and fill in real values
# Database
DATABASE_PATH=mylife.db
# TypeORM options
TYPEORM_SYNC=true
TYPEORM_LOGGING=true
# App
PORT=3000
ENABLE_CORS=true
# CORS / Frontend origin. Can be '*' or a comma-separated list like 'http://localhost:3000,http://localhost:3001'
FRONTEND_ORIGIN=http://localhost:3001
# JWT
JWT_SECRET=change_this_to_a_strong_secret
JWT_EXPIRES_IN=3600s
# Upload limits
MAX_IMAGE_SIZE_BYTES=5242880 # 5MB
MAX_VIDEO_SIZE_BYTES=52428800 # 50MB
